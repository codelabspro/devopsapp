<div class='container'>
	<h1><%= user.username %> </h1>
	<% if (user.admin) { %>
		<img src="/images/111-user.png">admin
	<% } else { %>
			<img src="/images/111-user.png">user
	<% } %>
	<h3><%= user.title %> </h3>
	<hr>
	<h3>contact: <%= user.email %> </h3>

	<a class="btn btn-medium btn-primary" href="/user/edit/<%= user.id %>">Edit</a>

</div>
